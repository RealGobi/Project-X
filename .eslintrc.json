{
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "airbnb"],
    "plugins": [
      "react"
    ],
    "globals": {
      "webkit": true
    },
    "rules": {
      "max-len": 0,
      "no-console": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-props-no-spreading": 0,
      "react/no-array-index-key": 0,
      "react/jsx-one-expression-per-line": 0,
      "jsx-a11y/alt-text": 0
    }
  }